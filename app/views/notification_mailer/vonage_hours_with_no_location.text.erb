Vonage Hours with No Location Attached

<% for timesheet in @timesheets do %>

<%= timesheet.shift_date.strftime('%m/%d/%Y') %>
<% connect_user = timesheet.connect_user %>
<%= connect_user.name %>
<%= connect_user.username %>
<% if not connect_user.connect_regions.empty? %>
<%= connect_user.connect_regions.map { |connect_region| connect_region.name }.join(', ') %>
<% elsif not connect_user.supervisor.connect_regions.empty? %>
<%= connect_user.supervisor.connect_regions.map { |connect_region| connect_region.name }.join(', ') %>
<% else %>
<% supervisor = connect_user.supervisor %>
<% end %>

<% while supervisor.supervisor and supervisor.firstname == 'X' %>
<% supervisor = supervisor.supervisor %>
<% if supervisor %>
<%= supervisor.name %>
<% else %>
<%= timesheet.customer %>
<%= timesheet.hours.round(2).to_s %>
<% end %>
<% end %>