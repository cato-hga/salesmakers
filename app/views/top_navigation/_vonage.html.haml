- vonage_sale_policy = VonageSalePolicy.new(@current_person, VonageSale.new)
- walmart_gift_card_policy = WalmartGiftCardPolicy.new(@current_person, WalmartGiftCard.new)
- retail = Project.find_by name: 'Vonage Retail'
- if @visible_projects.include?(retail) && (walmart_gift_card_policy.new? || vonage_sale_policy.new?)
  %li.has-dropdown
    %a{ href: '#' } Vonage Retail
    %ul.dropdown
      - if vonage_sale_policy.new?
        %li= link_to 'Sale Entry', new_vonage_sale_path
      - if walmart_gift_card_policy.new?
        %li.divider
        %li= link_to 'Gift Card Import/Balance Check', new_walmart_gift_card_path
